{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "DatasetGroupArn",
    "DatasetType",
    "Name",
    "SchemaArn"
  ],
  "properties": {
    "DatasetGroupArn": {
      "type": "string"
    },
    "DatasetType": {
      "type": "string",
      "enum": ["INTERACTIONS", "ITEMS", "USERS"]
    },
    "Name": {
      "type": "string"
    },
    "SchemaArn": {
      "type": "string"
    }
  }
}
